{{define "index"}}
    {{template "header"}}
        {{template "menu"}}

            <h2>Registered</h2>
                <table border="1">
                    <thead>
                        <tr>
                            <td> ID </td>
                            <td> NAME </td>
                            <td> CITY </td>
                            <td> EDIT </td>

                        </tr>
                    </thead>
                    <tbody>
                        {{range .}}
                        <tr>
                            <td>{{.Id}}</td>
                            <td>{{.Name}}</td>
                            <td>{{.City}}</td>
                            <td><a href="/edit?id={{.Id}}"> Edit </a></td>

                        </tr>
                        {{end}}
                    </tbody>
                </table>
    {{template "footer"}}
{{end}}
    